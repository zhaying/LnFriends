<!-- views/sa.handlebars -->

	<div class="container">
		<div class="page-header text-center">
			<h1><span class="fa fa-anchor"></span> SA Dashboard Home</h1>
			<a href="/dashboard" class="btn btn-light btn-sm">Home</a>
			<a href="/logout" class="btn btn-light btn-sm">Logout</a>
		</div>
		<div class="row">
			<!-- LOCAL INFORMATION -->
			<div class="col-sm-12">

				<h1>{{title}}</h1>

				<div class="row">
					<!-- LOCAL INFORMATION -->
					<div class="col-sm-6">

						<!-- websocket test -->
						<div id="coin_data_results" class="card mb-5">
							<h3><span class="fa fa-user"></span> Tickers</h3>
							<div id="tableCoinData" class="mb-5">
								<button id="btnGetCMC" class="btn btn-secondary" >Get CMC listings</button>
							</div>
							<input id="coinSymbol" type="text" placeholder="Enter coin symbol" />
							<button id="btnSendCoin"  class="btn btn-secondary">Refresh Price</button>
							<div id="output"></div>
						</div>

						<!-- JP- websocket test /* TheBSODPool *****-->
						<div id="total_data_results" class="card">
							<h3><span class="fa fa-user"></span> Data</h3>
							<div id="tableTotalData" class="mb-5">
								<button id="btnGetTotal" class="btn btn-secondary" >Get Total</button>
							</div>
							<input id="totalSymbol" type="text" placeholder="Enter coin symbol" />
							<button id="btnSendTotal"  class="btn btn-primary">Refresh Total</button>
							<div id="output"></div>
						</div>

					</div> <!-- ./end col-sm-6 -->
				</div> <!-- ./end row -->
			</div> <!-- ./end col-sm-12 -->
		</div> <!-- ./end row -->
	</div> <!-- ./end cointainer -->
