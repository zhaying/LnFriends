<!-- views/profile -->
<!doctype html>
<html>
<head>
	<title>suDashboard</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">

	<style>
		body 		{ padding-top:80px; word-wrap:break-word; }
	</style>
</head>
<body>
<div class="container">

	<div class="page-header text-center">
		<h1><span class="fa fa-anchor"></span> SA Dashboard Home</h1>
		<a href="/" class="btn btn-default btn-sm">Home</a>
		<a href="/logout" class="btn btn-default btn-sm">Logout</a>
	</div>

	<div class="row">

		<!-- LOCAL INFORMATION -->
		<div class="col-sm-6">
			<div class="well">
				<h3><span class="fa fa-user"></span> Local</h3>

					<p>
						<strong>id</strong>: user._id<br>
						<strong>email</strong>: user.local.email<br>
						<strong>password</strong>: user.local.password
					</p>


					${template}

				<button id="btnTest" >TEST API Button</button>
			</div>
			<!-- websocket test -->
			<div id="coin_data_results" class="well">
				<h3><span class="fa fa-user"></span> Data</h3>
				<div id="tableCoinData">
					<div id="output"></div>
				</div>
				<input id="coinSymbol" type="text" placeholder="Enter coin symbol" />
				<button id="btnSendCoin" >Refresh Price</button>
			</div>
		</div>

	</div>

</div>
<!-- <script src="assets/js/saDashboardScript.js"></script> -->
</body>
</html>
